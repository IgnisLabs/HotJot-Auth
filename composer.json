{
    "name": "ignislabs/hotjot-auth",
    "description": "Laravel JSON Web Token authentication library",
    "type": "library",
    "require": {
        "nesbot/carbon": "^1.22",
        "ignislabs/hotjot": "^0.1.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^4.0",
        "psr/http-message": "^1.0",
        "illuminate/http": "^5.4",
        "predis/predis": "^1.0",
        "illuminate/contracts": "^5.5",
        "illuminate/auth": "^5.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Luciano Longo",
            "email": "luciano.longo@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "IgnisLabs\\HotJot\\Auth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\IgnisLabs\\HotJot\\Auth\\": "spec/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IgnisLabs\\HotJot\\Auth\\Laravel\\HotJotAuthServiceProvider"
            ]
        }
    }
}
